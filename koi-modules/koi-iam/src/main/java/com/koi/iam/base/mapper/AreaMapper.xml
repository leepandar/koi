<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.koi.iam.base.mapper.AreaMapper">

    <select id="listArea" resultType="com.koi.iam.base.domain.entity.AreaEntity">
        SELECT t1.*
        FROM c_area t1
        where t1.parent_id = #{parentId}
        order by sequence
    </select>


</mapper>
